{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Boilerlate React template with Webpack. Supports SASS/ CSS modules.",
  "main": "index.js",
  "scripts": {
    "dev": " webpack-dev-server --env.env=development --config build-utils/webpack.config.js  --open",
    "build": "webpack --env.env=production --config build-utils/webpack.config.js  ",
    "build:dev": "cd dist && http-server -o index.html -p 3000",
    "format": "prettier --write \"src/**/*.{js,jsx}\" \"build-utils/**/*.{js,jsx}\" ",
    "lint": "eslint \"src/**/*.{js,jsx}\" \"build-utils/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.17.0",
